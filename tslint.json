{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "eofline": false,
        "indent": [
            true,
            "tabs",
            4
        ],
        "no-trailing-whitespace": [
            true,
            "ignore-comments"
        ],
        "object-literal-sort-keys": false,
        "quotemark": [
            false
        ],
        "trailing-comma": [
            true,
            {
                "multiline": "never",
                "singleline": "never"
            }
        ]
    },
    "rulesDirectory": [
        "node_modules/tslint-eslint-rules/dist/rules"
    ]
}